    "def is_leap(year):\n",
    "    if year % 4 != 0 or (year % 100 == 0 and year % 400 != 0):\n",
    "        return False\n",
    "    else:\n",
    "        return True\n",
    "    \n",
    "        \n",
    "year = int(input())\n",
    "print(is_leap(year))"
